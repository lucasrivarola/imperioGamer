                  <button type="sumbit" class="btn btn-eliminar">Elminar</button>
                </form>
              </div>
              
          </div>
        </div>
        <% });%> 
      </div>
      <div class="row">
        <div class="col-12">
          <h3 id="accesorios">Accesorios</h3>
        </div>
        <%accesorios.forEach(function(accesorios){%>
