                  <form action="/products/<%= consolas.id %>?_method=DELETE" method="POST">
                    <button type="sumbit" class="btn btn-eliminar">Elminar</button>
                  </form>
                </div>
                
            </div>
          </div>
        <% });%> 
      </div>
      <div class="row">
        <div class="col-12">
          <h3 id="pcGamer">Pc Gamer</h3>
        </div>
        <%pcGamer.forEach(function(gamer){%>
          <div class="col-xs-12 col-sm-6 col-md-6 col-lg-3">
