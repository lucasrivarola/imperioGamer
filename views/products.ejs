                <form action="/products/<%= gamer.id %>?_method=DELETE" method="POST">
                  <button type="sumbit" class="btn btn-eliminar">Elminar</button>
                </form>
              </div>
              
          </div>
        </div>
        <% });%> 
      </div>
      <div class="row">
        <div class="col-12">
          <h3 id="sillas">Sillas</h3>
        </div>
        <%sillas.forEach(function(sillas){%>
          <div class="col-xs-12 col-sm-6 col-md-6 col-lg-3">
            <div class="card" >
              <a class="productAdmin" href="/user/detail/<%= sillas.id %> ">
                <h5 class="card-title productTitle"><strong><%= sillas.product_name %></strong> </h5>
