<!DOCTYPE html>
<html>
  <%- include('./partials/head') %> 
  <link href="/stylesheets/productoAdmin.css" rel="stylesheet">
  <body>
    <%- include('./partials/header') %>
    <div class="container">
      <div class="row">
        <div class="col-12 categorias">
          <ul>
            <li><a href="#juegos">Juegos</a></li>
            <li><a href="#consolas">Consolas</a></li>
            <li><a href="#pcGamer">Pc Gamer</a></li>
            <li><a href="#sillas">Sillas</a></li>
            <li><a href="#accesorios">Accesorios</a></li>
          </ul>
        </div>
      </div>
      <div class="row">
        <div class="col-12">
          <h3 id="juegos">Juegos</h3>
        </div>

          <%juegos.forEach(function(juegos){%>
            <div class="col-xs-12 col-sm-6 col-md-6 col-lg-3">
              <div class="card" >
                <a class="productAdmin" href="/user/detail/<%= juegos.id %> ">
                  <h5 class="card-title productTitle"><strong><%= juegos.product_name %></strong> </h5>
                  <img width="300" height="200" src="/images/productos/<%= juegos.image %>" class="card-img-top" alt="<%= juegos.name %> ">
                </a>
                <div class="card-body">
                  <form method="GET" action="/products/<%= juegos.id%>/edit">
                    <button type="sumbit" class="btn btn-editar">Editar</button>
                  </form>
                  <form action="/products/<%= juegos.id %>?_method=DELETE" method="POST">
                    <button type="sumbit" class="btn btn-eliminar">Elminar</button>
                  </form>
                </div>
                
            </div>
          </div>
          <% });%> 

      </div>

      <div class="row">
        <div class="col-12">
          <h3 id="consolas">Consolas</h3>
        </div>
        <%consolas.forEach(function(consolas){%>
          <div class="col-xs-12 col-sm-6 col-md-6 col-lg-3">
